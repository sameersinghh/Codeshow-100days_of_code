#include<iostream>
using namespace std;

int main(){

 int n;  //initialisation of variable
 cout<<"Please input a number \n";  //asking user for input 
 cin>>n;  //taking input

 for (int i = n; i >=1; i--)
 {
     for (int j = 1; j<=i; j++)
     {
         cout<<j<<" ";
     }
     cout<<endl;
 }
 
    return 0;
}
