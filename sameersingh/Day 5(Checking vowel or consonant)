#include<iostream>
using namespace std;

int main(){

 char c;
 int lowercasevowel, uppercasevowel;
 cin>>c;

 lowercasevowel=(c=='a'|| c=='e'|| c=='i'|| c=='o'||  c=='u');

 uppercasevowel=(c=='A'|| c=='E'|| c=='I'|| c=='O'||  c=='U'); 

 if (lowercasevowel || uppercasevowel)
 {
     cout<<c<<" is a vowel";
 }
 else
 {
     cout<<c<<" is a consonant";
 }

     return 0;
}
