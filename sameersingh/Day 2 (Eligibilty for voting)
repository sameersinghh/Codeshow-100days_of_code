#include <iostream>
using namespace std;

int main(){

 int age; //intialisation of variable
 cout<<"Please input age to eligibilty"<<endl; //Asking user for input
 cin>>age; //taking input numbers

 if (age>=18)  //condition for comparison
 {
     cout<<"Eligible to vote";
 }
 else
 {
      cout<<"Not eligible to vote";
 }

    return 0;
}
