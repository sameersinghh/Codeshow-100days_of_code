#include<iostream>
using namespace std;

int main(){

 int n;  //initialisation of variable
 cout<<"Please input a number \n";  //asking user for input
 cin>>n;  //taking input

 int i;

 for (int i = 2; i <=n ; i++)
   {
       if (n%i==0)  //condition to check whether prime or not
       {
           cout<<"Number is non-prime"<<endl;
           break;
       }     
   }
 if (i==n)  //to ensure that loop is completed first
 {
     cout<<"Number is prime"<<endl;
 }
    return 0;
}
